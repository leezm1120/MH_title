.searchbar-result {
  margin-top: 0;
  font-size: 14px;
}

.searchbar-result:before {
  display: none;
}

.weui-cell {
  padding: 10px 20px 10px 20px;
  font-size: 14px;
}

.desc-cell {
  padding: 0px 40px 10px 40px;
  font-size: 14px;
  color: #666;
}

